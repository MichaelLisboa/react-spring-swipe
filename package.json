{
    "name": "swiper",
    "version": "0.1.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/MichaelLisboa/react-spring-swipe.git"
    },
    "homepage": "https://swiper.hypermix.com",
    "dependencies": {
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "axios": "^0.21.0",
        "concurrently": "^5.3.0",
        "gh-pages": "^3.1.0",
        "nodemon": "^2.0.6",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-scripts": "4.0.0",
        "react-spring": "^8.0.27",
        "react-use-gesture": "^8.0.0",
        "web-vitals": "^0.2.4",
        "yes-https": "^2.0.0"
    },
    "scripts": {
        "predeploy": "npm run generate-build-version && npm run build",
        "deploy": "gh-pages -d build",
        "generate-build-version": "node generate-build-version",
        "start": "node server.js",
        "build": "react-scripts build",
        "server": "nodemon server.js",
        "local": "concurrently \"npm run server\" \"react-scripts start\""
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "http://localhost:5000"
}
